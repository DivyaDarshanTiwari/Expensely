{
  "expo": {
    "name": "ExpenselyExpo",
    "slug": "ExpenselyExpo",
    "owner": "vilsium",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expenselyexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./google-services.json",
        "package": "com.application.expensely"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.vilsium.ExpenselyExpo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "54f5ac2b-ff86-4fb3-bb33-e012d2d4b0c6"
      },
      "Basic_URL": "https://awake-fascination-production.up.railway.app",
      "Group_URL": "https://satisfied-solace-production.up.railway.app",
      "User_URL": "https://lucky-vitality-production.up.railway.app",
      "OCR_URL": "https://vigilant-tranquility-production.up.railway.app"
    }
  }
}
